Teisendamine

let minutid = 0
let tunnid = 0
let p채evad = 0
i = parseFloat(prompt())

function teisendamine() {
    let minutid = i / 60
    let tunnid = minutid / 60
    let p채evad = tunnid / 24

    console.log(Math.floor(minutid) + " minuteid")
    console.log(Math.floor(tunnid) + " tunde")
    console.log(Math.floor(p채evad) + " p채eva")

}

teisendamine()

